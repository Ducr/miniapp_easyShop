.tab-title{
  display: flex;
  height: 90rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid transparent;
}
.tab-item{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.tab-item.active{
  color:var(--themeColor);
  border-bottom: 2rpx solid currentColor;
}